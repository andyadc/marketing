				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.RuleTreeNode"/>
		<typeAlias alias="RuleTreeNodeLine"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.RuleTreeNodeLine"/>

		<!-- seckill -->
		<typeAlias alias="SeckillGoods"
				   type="com.andyadc.marketing.infrastructure.persistence.seckill.po.SeckillGoods"/>
		<typeAlias alias="SeckillUser"
				   type="com.andyadc.marketing.infrastructure.persistence.seckill.po.SeckillUser"/>
		<typeAlias alias="SeckillActivity"
				   type="com.andyadc.marketing.infrastructure.persistence.seckill.po.SeckillActivity"/>
