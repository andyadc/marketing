	<typeAliases>
		<typeAlias alias="Award"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.Award"/>
		<typeAlias alias="Strategy"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.Strategy"/>
		<typeAlias alias="StrategyAward"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.StrategyAward"/>
		<typeAlias alias="StrategyRule"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.StrategyRule"/>
		<typeAlias alias="DrawActivity"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.DrawActivity"/>
		<typeAlias alias="DrawActivitySku"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.DrawActivitySku"/>
		<typeAlias alias="DrawActivityAccount"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.DrawActivityAccount"/>
		<typeAlias alias="DrawActivityAccountFlow"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.DrawActivityAccountFlow"/>
		<typeAlias alias="DrawActivityCount"
				   type="com.andyadc.marketing.infrastructure.persistence.marketing.po.DrawActivityCount"/>
